{
  "family": "health-api-task-family",
  "taskRoleArn": "arn:aws:iam::336449586386:role/ecsTaskExecutionRole",
  "executionRoleArn": "arn:aws:iam::336449586386:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "containerDefinitions":,
      "essential": true,
      "environment": [
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "production"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/health-api",
          "awslogs-region": "ap-south-1",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ],
  "requiresCompatibilities": [
    "FARGATE",
    "EC2"
  ],
  "placementConstraints": []
}
